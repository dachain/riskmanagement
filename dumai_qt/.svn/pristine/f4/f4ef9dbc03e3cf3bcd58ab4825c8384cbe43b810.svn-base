body{
    font-size: 16px;
}
body,html{
    width:100%;
    height:100%;
    overflow:hidden;
    margin: 0;
    padding: 0;
    color: white;
    font-family: "微软雅黑", "Microsoft YaHei";
}
ul li{
    list-style: none;
}
input::-webkit-inner-spin-button {
    -webkit-appearance: none;
}
.allContent input{
    display: inline-block;
    border: none;
    background: none;
    outline: none;
    -webkit-tap-highlight-color:rgba(255,0,0,0);
}
.allContent #stars {
    width: 100%;
    height: 100%;
    z-index:-1;
    position: absolute;
    background:url("../images/beijing.png");
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    user-select: none;
}
.allContent #logo{
    width: 20%;
    height: 50px;
    border: 1px solid blue;

}
.allContent .circle_pure{
    position: absolute;
    left:0;
    right:0;
    top:0;
    bottom: 0;
    margin: auto;
    width: 372px;
    height:372px;
    border-radius: 50%;
    background: url("../images/yuan_chushi.png") no-repeat;
    z-index: 2;
}
.allContent .on{
    background: url("../images/yuan_huaguo.png") no-repeat;
}
.allContent #circle{
    position: absolute;
    left:0;
    right:0;
    top:0;
    bottom: 0;
    margin: auto;
    width: 435px;
    height:435px;
    border-radius: 50%;
    background: url("../images/yuan_beijing.png") no-repeat;
}
.allContent .circle_pure ul{
    position: absolute;
    width: 100%;
    height: 100%;
    padding-top: 36px;
    padding-left:68px;

}
.allContent .circle_pure ul li{
    text-align: left;
    width: 230px;
    height: 36px;
    line-height: 45px;
    padding-left: 10px;
    margin-top: 20px;

}
.allContent .user_load,.password_load{
    color: #fdffff;
    border-bottom: 1px solid #3fa6de;
    letter-spacing:5px;
}
.allContent .yam_num{
    padding: 0;
    font-size: 12px;
    text-align: center;
    color: white;
}
.allContent .circle_pure ul li i{
    display: inline-block;
    width: 20px;
    height: 20px;
    margin-bottom: 10px;
    vertical-align: middle;
    margin-right: 20px;
}
.allContent .password,.user{
    color: #6d7e89;
    width: 150px;
    font-size: 16px;
    font-weight: 500;
    letter-spacing:10px;
}
.allContent .sub{
    width: 240px;
    height: 36px;
    line-height: 36px;
    background: #3fa6de;
    border-radius: 5%;
    color: #021622;
    font-size: 18px;
    padding-left: 42px;
    letter-spacing:42px;
    cursor: pointer;

}
.allContent .che_ck{
    display: none;
}

.che_ck, .load_label {
    background-color: #3fa6de;
    box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05), inset 0px -15px 10px -12px rgba(0, 0, 0, 0.05);
    padding: 8px;
    display: inline-block;
    margin-right: 10px;
    vertical-align:middle;
}
.load_label{
    margin-top: 3px;
}

.icon_user{
    background:url("../images/user.png") no-repeat;
    background-size: cover;
}
.icon_password{
    background:url("../images/icon_password.png") no-repeat;
    background-size: cover;
}
.circle_pure ul li.yzm{
    padding-left: 0px;
    letter-spacing: 10px;
    line-height: 38px;
    color: white;

}
.yzm div,.yzm input{
    letter-spacing: 2px;
    display: inline-block;
    width: 87px;
    height: 36px;
    border: 1px solid gray;
    padding-left: 10px;
}

.yzm input{
    position:absolute;
    left:155px;
    width: 46px;
    height: 36px;
    text-align: center;
    font-size: 12px;
}
.yzm a{
    float: right;
    text-decoration: none;
    font-size: 14px;
    letter-spacing: 5px;
    color: white;

}
.yzm a:hover,.yzm a:focus{
     color:white;
     text-decoration: none;
}
/*.yzm a:hover{*/
    /*text-decoration: none;*/
    /*color: white;*/
/*}*/
.extra{
    padding-left: 0;
    position: absolute;
    font-family: "微软雅黑";
    letter-spacing:2px;
    font-size: 14px;
    cursor: pointer;
    color: white;

}
.extra a{
    color: white;
    float: right;
    text-decoration: none;
}
.extra a:hover{
    text-decoration: underline;
}
.extra img{
    position: absolute;
    width: 14px;
    height: 14px;
    display: inline-block;
    left:11px;
    top:15px;
}
.tip_error,.tip_error2,.tip_error3{
    position: absolute;
    left:0;
    right:0;
    bottom:0;
    top:49px;
    margin: auto;
    width: 252px;
    height: 104px;
    padding-top: 27px;
    text-align: center;
    word-wrap : break-word ;
    color: #ef0606;
    background:url("../images/tip_error.png") no-repeat ;

}
.tip_error2{
    top:169px;
}
.tip_error .close,.tip_error2 .close,.tip_error3 .close{
    position: absolute;
    top:8px;
    right:15px;
    display: inline-block;
    width: 20px;
    height: 20px;
    background: url("../images/close.png") no-repeat;
    cursor: pointer;
}