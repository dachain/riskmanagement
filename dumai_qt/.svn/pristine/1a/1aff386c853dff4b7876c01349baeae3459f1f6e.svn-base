body,html{
    width: 100%;
    height: 100%;
    font-family: "Î¢ÈíÑÅºÚ";
}
*{
    margin: 0;
    padding: 0;
}
ul li{
    list-style: none;
}

.messbox {
    position: absolute;
    border: 2px solid #3eaf28;
    width: 99%;
    height: 99%;
   left:5px;
    border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    box-shadow: 0 0 5px #6b8466;
    -moz-box-shadow: 0 0 5px #6b8466;
    -webkit-box-shadow: 0 0 5px #6b8466;
}
.warntitle {
    width: 100%;
    height: 35px;
    background: #3eaf28;
}

.warntitle h4{
    display: inline-block;
    font-size: 14px;
    line-height: 35px;
    padding-left: 5px;
    margin: 0;
}
.warntitleRight{
    display: inline-block;
    float: right;
    margin-right: 40px;
    line-height: 35px;
}
.warnContent {
    width: 100%;
    height: 94%;
    background: white;
}

.warnContent .selectLi{
    width: 40%;
    height: 100%;
}
.warnContent .selectLi li {
    width: 100%;
    line-height: 28px;
    text-align: center;
    font-size: 14px;
    border: 1px solid #ccc;
    cursor: pointer;
}
.warnContent .title{
    width: 40%;
    height: 40px;
    line-height: 40px;
    padding-left: 15px;
    background: lightgrey;
}
.title span{
    display: inline-block;
    width: 22%;
    text-align: left;
}
.warnContent .selectLi li:nth-child(1){
    background:#65bf53;
    color: #fff;
}
.warnContent .selectLi li span{
    display: inline-block;
    width: 20%;
    height: 100%;
    white-space:normal;
    margin-right: 15px;
    text-align: left;
}
.rightBottom{
    position: absolute;
    width: 60%;
    height:96%;
    top:35px;
    right:0;
}

.messbtn {
    position:absolute;
    cursor: pointer;
    border: 1px solid #2e9d17;
    width: 35px;
    height: 35px;
    background: url(../../../img/icon.png) no-repeat -89px -73px;
    top:-2px;
    right:0;
}